local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Blockman go admin panel", "DarkTheme")
local Tab = Window:NewTab("Hack")
local Section = Tab:NewSection("Hacks")
Section:NewButton("inf jump", "  ", function()
local InfiniteJumpEnabled = true game:GetService("UserInputService").JumpRequest:connect(function() 	if InfiniteJumpEnabled then 		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping") 	end end)
print("Clicked")
end)
Section:NewButton("reach and hitbox", "  ", function()
loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-Update-script-hitbox-9326"))()
print("Clicked")
end)
Section:NewButton("AtackCD", "  ", function()
g = hookfunction(wait, function(seconds) 
return g(0)
end)
print("Clicked")
end)
Section:NewButton("free cam", "   ", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/zephyr10101/CameraSpy/main/Script", true))()
print("Clicked")
end)
Section:NewButton("dev fly", "   ", function()
loadstring(game:HttpGet("https://pastebin.com/raw/eR4bgsRp"))();
print("Clicked")
end)
Section:NewButton("Speed", "   ", function()
Game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 80
print("Clicked")
end)
Section:NewButton("High jump", "   ", function()
Game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
print("Clicked")
end)
Section:NewButton("aimbot", "   ", function()
loadstring(game:HttpGet("https://rentry.co/n55gmtpi/raw", true))()
print("Clicked")
end)
Section:NewButton("clicktp", "   ", function()
mouse = game.Players.LocalPlayer:GetMouse() tool = Instance.new("Tool") tool.RequiresHandle = false tool.Name = "Equip to Click TP" tool.Activated:connect(function() local pos = mouse.Hit+Vector3.new(0,2.5,0) pos = CFrame.new(pos.X,pos.Y,pos.Z) game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos end) tool.Parent = game.Players.LocalPlayer.Backpack
print("Clicked")
end)
Section:NewButton("noclip", "   ", function()
loadstring(game:HttpGet("https://pastebin.com/raw/kG3CJf1f"))();
print("Clicked")
end)
local Section = Tab:NewSection("the end of the hacks")
local Tab = Window:NewTab("Effects")
local Section = Tab:NewSection("Epic effects!!!!!!!")
Section:NewButton("Epic Effects", "   ", function()
loadstring(game:HttpGet(('https://pastefy.app/xXkUxA0P/raw'),true))()
print("Clicked")
end)
local Tab = Window:NewTab("Items")
local Section = Tab:NewSection("ONLY YOU CAN SEE IT")
Section:NewButton("ITEMS", " ONLY YOU CAN SEE IT  ", function()
loadstring(game:HttpGet("https://pastebin.com/raw/AZVi2tuK"))()
print("Clicked")
end)
local Tab = Window:NewTab("Credits")
local Section = Tab:NewSection("Made by Haxarebest1")
local Section = Tab:NewSection("this script is a admin panel from blockman go")
local Tab = Window:NewTab("open/close script and keyboard")
local Section = Tab:NewSection("close/open if you want to close it press f on your keyboard")
Section:NewButton("keyboard", "   ", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
print("Clicked")
end)
Section:NewKeybind("KeybindText", "KeybindInfo", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
